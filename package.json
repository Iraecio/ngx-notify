{
  "name": "ng-snotify",
  "description": "Angular 9+ notifications center",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run link:src && ng serve",
    "example:build": "npm run lib:build && npm run link:dist && ng serve --aot --prod --open",
    "example:build:prod": "npm run lib:build && npm run link:dist && ng build --aot --prod --base-href=/ng-snotify/  --output-path=demo/",
    "example:test": "ng test",
    "example:lint": "ng lint",
    "example:e2e": "ng e2e",
    "build": "gulp build",
    "build:watch": "gulp",
    "link:src": "npm link ./src",
    "link:dist": "npm link ./dist",
    "compodoc": "npm run compodoc:build",
    "compodoc:build": "compodoc -p tsconfig.docs.json -n ng-snotify -d -d demo/compodoc/ --hideGenerator --theme Laravel",
    "compodoc:serve": "npm run compodoc:build -- -s",
    "compodoc:watch": "npm run compodoc:build -- -s -w",
    "lint": "tslint --type-check -p src/tsconfig.es5.json src/**/*.ts",
    "test": "npm run link:dist && ng test",
    "lib:lint": "npm run lint && npm run example:lint",
    "lib:build": "npm run lib:lint && npm run build",
    "docs:build": "gitbook build ./docs ./demo/documentation",
    "docs:deploy": "bash ./update-docs.sh",
    "prepare": "npm run build"
  },
  "homepage": "https://artemsky.github.io/ng-snotify",
  "repository": {
    "type": "git",
    "url": "https://github.com/artemsky/ng-snotify"
  },
  "authors": [
    {
      "name": "artemsky",
      "email": "mr.artemsky@gmail.com"
    },
    {
      "name": "iraecio",
      "email": "iraecio@gmail.com"
    }
  ],
  "keywords": [
    "angular",
    "angular 9",
    "Angular9",
    "ng",
    "ng9",
    "9",
    "Library",
    "Notifications",
    "Notification",
    "Toast",
    "toasts",
    "promt",
    "async",
    "confirmation",
    "notify",
    "notie",
    "notification-center",
    "snotify",
    "ng-snotify",
    "ng2-snotify",
    "ngx2-snotify",
    "angular9 notifications",
    "toaster"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://artemsky.github.io/ng-snotify/issues"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.5",
    "@angular/animations": "^9.0.5",
    "@angular/cli": "^9.0.5",
    "@angular/common": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/http": "^7.2.16",
    "@angular/language-service": "^9.0.5",
    "@angular/platform-browser": "^9.0.5",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@angular/router": "^9.0.5",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.5.8",
    "@types/node": "^13.9.0",
    "bootstrap": "^4.4.1",
    "codelyzer": "^5.2.1",
    "core-js": "^3.6.4",
    "del": "^5.1.0",
    "event-stream": "^4.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-rollup": "^2.17.0",
    "gulp-sass": "^4.0.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "node-sass": "^4.13.1",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.6.3",
    "protractor": "~5.4.3",
    "rollup": "^2.0.2",
    "run-sequence": "^2.2.1",
    "rxjs": "^6.5.4",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "zone.js": "^0.10.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
